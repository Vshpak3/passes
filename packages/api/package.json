{
  "name": "@moment/api",
  "version": "1.0.0",
  "description": "Moment API package",
  "private": true,
  "scripts": {
    "build": "nest build",
    "docker": "docker compose up --detach",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "NODE_ENV=dev nest start --watch --debug",
    "migration:create": "yarn mikro-orm migration:create",
    "migration:up": "yarn mikro-orm migration:up",
    "migration:down": "yarn mikro-orm migration:down",
    "generate-openapi-spec": "NODE_ENV=dev yarn ts-node --project tsconfig.json ./src/generate-openapi-spec.ts",
    "lint:check": "eslint . --max-warnings=0",
    "lint:fix": "eslint --fix .",
    "prettier:check": "prettier --check . || { echo; echo 'run prettier:fix to fix this issue'; exit 1; }",
    "prettier:fix": "prettier --write .",
    "ts:check": "tsc --noEmit --project tsconfig.json",
    "test": "NODE_ENV=dev jest --forceExit",
    "test:watch": "NODE_ENV=dev jest --watch",
    "test:cov": "NODE_ENV=dev jest --coverage"
  },
  "dependencies": {
    "@jest/globals": "28.1.2",
    "@mikro-orm/cli": "5.2.2",
    "@mikro-orm/core": "5.2.2",
    "@mikro-orm/migrations": "5.2.2",
    "@mikro-orm/nestjs": "5.0.2",
    "@mikro-orm/postgresql": "5.2.2",
    "@mikro-orm/reflection": "5.2.2",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs-modules/ioredis": "1.0.1",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "5.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "express": "4.18.1",
    "ioredis": "5.1.0",
    "joi": "17.6.0",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "swagger-ui-express": "4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.7",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.1",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.4",
    "@typescript-eslint/parser": "5.30.4",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-regexp": "1.7.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/database/mikro-orm.config.ts",
      "./dist/src/database/mikro-orm.config.js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
